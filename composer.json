{
    "name": "dmk/mktools",
    "description": "Collection of some small tools.",
    "version": "9.5.4",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3 CMS",
        "tools"
    ],
    "homepage": "http://www.dmk-ebusiness.de/",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "DMK E-Business GmbH",
            "email": "dev@dmk-ebusiness.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "dev@dmk-ebusiness.de",
        "source": "https://github.com/DMKEBUSINESSGMBH/typo3-mktools",
        "issues": "https://github.com/DMKEBUSINESSGMBH/typo3-mktools"
    },
    "replace": {
        "typo3-ter/mktools": "self.version"
    },
    "require": {
        "typo3/cms-core": "~8.7 || ~9.5",
        "digedag/rn-base": ">=1.11.0",
        "dmk/mklib": ">=9.5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2@stable",
        "nimut/testing-framework": "^4.0"
    },
    "autoload": {
        "classmap": [
            "action/",
            "hook/",
            "model/",
            "scheduler/",
            "util/",
            "view/",
            "xclasses/",
            "Classes/"
        ],
        "psr-4": {
            "DMK\\Mktools\\" : "Classes"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "preferred-install": {
            "typo3/cms": "source"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "mkdir -p .Build/Web/typo3conf/ext/",
            "[ -L .Build/Web/typo3conf/ext/mktools ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/mktools"
        ]
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "extension-key": "mktools",
            "web-dir": ".Build/Web"
        }
    }
}
