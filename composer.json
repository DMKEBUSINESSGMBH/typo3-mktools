{
    "name": "dmk/mktools",
    "description": "Collection of some small tools.",
    "version": "3.0.29",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3 CMS",
        "tools"
    ],
    "homepage": "http://www.dmk-ebusiness.de/",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "DMK E-Business GmbH",
            "email": "dev@dmk-ebusiness.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "dev@dmk-ebusiness.de",
        "source": "https://github.com/DMKEBUSINESSGMBH/typo3-mktools",
        "issues": "https://github.com/DMKEBUSINESSGMBH/typo3-mktools"
    },
    "replace": {
        "typo3-ter/mktools": "self.version"
    },
    "require": {
        "typo3/cms": "~4.5 || ~6.2 || ~7.6 || ~8.7",
        "digedag/rn-base": ">=1.5.1",
        "dmk/mklib": ">=3"
    },
    "autoload": {
        "classmap": [
            "action/",
            "hook/",
            "model/",
            "scheduler/",
            "util/",
            "view/",
            "xclasses/",
            "Classes/"
        ],
        "psr-4": {
            "DMK\\Mktools\\" : "Classes"
        }
    }
}
